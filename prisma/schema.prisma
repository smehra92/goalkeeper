// prisma/schema.prisma
datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Stuff {
  id        String   @id @default(uuid())
  post      String
  isEditing Boolean @default(false)
  createdAt DateTime @default(now())
}